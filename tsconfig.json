{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "jsx": "react",
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "noLib": false,
    "outDir": "./dist",
    "preserveConstEnums": true,
    "reactNamespace": "React",
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "es6"
  },
  "compileOnSave": true
}
